{
  "name": "virtual-file-system-v3-be",
  "version": "1.0.0",
  "license": "ISC",
  "description": "",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.compile.json && tsc-alias",
    "barrels": "barrelsby --config .barrelsby.json -q",
    "start": "yarn barrels && cross-env NODE_ENV=development tsnd --ignore-watch node_modules --respawn --transpile-only --max-old-space-size=2048 -r tsconfig-paths/register src/index.ts",
    "db:migrate": "npx prisma migrate dev --preview-feature",
    "db:reset": "npx prisma migrate reset --force",
    "db:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "lint": "eslint '**/*.{ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix",
    "format": "prettier '**/*.{ts,js,json}' --write",
    "prepare": "is-ci || husky install",
    "bootstrap": "yarn && yarn start:docker",
    "start:docker": "docker compose -f docker-compose.dev.yml up -d",
    "clean:docker": "docker compose -f docker-compose.dev.yml down --volumes --remove-orphans",
    "clean:git": "git branch --merged >/tmp/merged-branches && nano /tmp/merged-branches && xargs git branch -D </tmp/merged-branches && git fetch --prune --all",
    "reset:docker": "yarn clean:docker && yarn start:docker"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@prisma/client": "^5.12.0",
    "@sinclair/typebox": "^0.31.17",
    "ajv": "^8.12.0",
    "ascii-table3": "^0.8.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "oracledb": "^6.8.0",
    "prisma": "^5.12.0",
    "virtual-file-system-v3-be": "file:"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.9",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "barrelsby": "^2.8.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
